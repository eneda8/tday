    <link rel="icon" type="image/png" sizes="16x16" href="/favicon_io/favicon-16x16.png">
    <link rel="stylesheet" type="text/css" href="/stylesheets/stars/stars.css"/>
    <link rel="stylesheet" type="text/css" href="/stylesheets/styles.css"/>
    <link rel="stylesheet" href="/stylesheets/navbar.css">
    <link rel='stylesheet' href='/stylesheets/<%=style%>.css' />
    <% if((title === "Write / t'day") || (title === "Journal / t'day") || (title === "Edit Journal / t'day") ) {%> 
    <link rel="stylesheet" type="text/css" href="/stylesheets/journals/write.css"/>
    <% } else {%> 
    <link rel="stylesheet" type="text/css" href="/stylesheets/paper.css"/>
    <% } %> 
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;500&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Courgette&display=swap" rel="stylesheet">
